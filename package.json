{
  "name": "dcs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cli": "tsx cli/index.ts",
    "fetch": "tsx cli/index.ts",
    "update": "tsx cli/index.ts update",
    "remove": "tsx cli/index.ts remove",
    "mcp": "tsx mcp/server.ts",
    "mcp:build": "npx tsc mcp/server.ts --outDir mcp --target es2022 --module esnext --moduleResolution bundler --allowSyntheticDefaultImports && chmod +x mcp/server.js && cp mcp/server.js mcp/server",
    "mcp:node": "node mcp/server.js",
    "mcp:test": "node mcp/test.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^22.8.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tsx": "^4.7.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
